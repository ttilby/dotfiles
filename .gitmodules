[submodule ".tmux/plugins/tmux-mem-cpu-load"]
	path = .tmux/plugins/tmux-mem-cpu-load
	url = https://github.com/thewtex/tmux-mem-cpu-load.git
[submodule ".themes/zeta-zsh-theme"]
	path = .themes/zeta-zsh-theme
	url = https://github.com/skylerlee/zeta-zsh-theme.git
[submodule ".themes/zsh-spaceship-prompt"]
	path = .themes/zsh-spaceship-prompt
	url = https://github.com/denysdovhan/spaceship-prompt.git
[submodule "apps/diff-so-fancy"]
	path = apps/diff-so-fancy
	url = https://github.com/so-fancy/diff-so-fancy.git
[submodule "dotbot"]
	path = dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule "dotbot-apt"]
	path = dotbot-apt
	url = https://github.com/bryant1410/dotbot-apt
